---
layout: layouts/base.ejs
---

<div class="container mx-auto px-4">
    <h1 class="font-bold text-primary text-4xl"><%= title %></h1>
    <p class="mt-6 text-gray-700">Tap or click on photo to open in it's own page.</p>
    <div class="fourps mt-16 lg:flex lg:justify-evenly lg:w-full lg:flex-wrap">
        <div class=" w-full lg:w-5/12">
            <a href=" <%= img1L %>">
                <img src=" <%= img1 %>" alt="" srcset="" class=" w-full">
            </a>
        </div>

        <div class=" w-full lg:w-5/12">
            <a href=" <%= img2L %>">
                <img src=" <%= img2 %>" alt="" srcset="" class=" w-full mt-8 lg:mt-0">
            </a>
        </div>

        <div class=" w-full lg:w-5/12">
            <a href=" <%= img3L %>">
                <img src=" <%= img3 %>" alt="" srcset="" class=" w-full mt-8">
            </a>
        </div>

        <div class=" w-full lg:w-5/12">
            <a href=" <%= img4L %>">
                <img src=" <%= img4 %>" alt="" srcset="" class=" w-full mt-8">
            </a>
        </div>
    </div>
    <div class="link-back mt-8">
        <a href="/gallery/" class=" text-gray-700 text-2xl font-medium">Back to Project Gallery Page</a>
    </div>
</div>

<script src="../js/layout.js"></script>